{
  "getSchema": {
    "schema": {
      "method": "GET",
      "host": "<%= iparam.subdomain %>.freshdesk.com",
      "path": "/api/v2/custom_objects/schemas",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.api_key) %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getData": {
    "schema": {
      "method": "GET",
      "host": "<%= iparam.subdomain %>.freshdesk.com",
      "path": "/api/v2/custom_objects/schemas/<%= context.schema_id %>/records?ticketnummer=<%= context.ticketnummer %>",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.api_key) %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getCartPositionsData": {
    "schema": {
      "method": "GET",
      "host": "<%= iparam.subdomain %>.freshdesk.com",
      "path": "/api/v2/custom_objects/schemas/<%= context.schema_id %>/records?auftragsnummer=<%= context.auftragsnummer %>",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.api_key) %>",
        "Content-Type": "application/json"
      }
    }
  }
  
}